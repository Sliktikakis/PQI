<% h = 0 %>
<% f = 0 %>
<% ids = [] %>
<% @rencs.each do |r| %>
  <% u = Usager.find(r.usager_id) %>
  <% if !ids.include?(u.id) %>
    <% if u.sexe == "Mr" %>
    	<% h += 1 %>
    <% else %>
    	<% f += 1 %>
    <% end %>
    <% ids << u.id %>
  <% end %>
<% end %>

<% if f+h == 0 %>
  0
<% else %>
  <%= "#{f+h} (#{h}/#{f})" %>
<% end %>

<% if @ville.nil? && !@dates.nil? %>
  <% @f_dif = @f_dif + f %>
  <% @h_dif = @h_dif + h %>
<% end %>