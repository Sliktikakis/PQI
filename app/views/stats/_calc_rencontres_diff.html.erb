<% h = 0 %>
<% f = 0 %>
<% @usagers.each do |u| %>
  <% d = f+h %>
  <% unless u.rencontres.nil? %>
  	<% l = u.rencontres.split(' - ') %>
  	<% i = 0 %>
  	<% until f+h == d + 1 || i == l.length %>
  	  <% n = l[i].split('/').join('') %>
  	  <% if n.to_i > @dates.first.split('-').join('').to_i - 20000000 && n.to_i < @dates.last.split('-').join('').to_i - 20000000 %>
  	  	<% if u.sexe == "Mr" %>
  	  		<% h = h + 1 %>
  	  	<% else %>
  	  		<% f = f + 1 %>
  	  	<% end %>
  	  <% end %>
  	  <% i = i + 1 %>
    <% end %>
  <% end %>
<% end %>
<% if f+h == 0 %>
  0
<% else %>
  <%= "#{f+h} (#{h}/#{f})" %>
<% end %>

<% if @ville.nil? && !@dates.nil? %>
  <% @f_dif = @f_dif + f %>
  <% @h_dif = @h_dif + h %>
<% end %>